<fieldset class="fieldset" id="report">
	<div class="row-fluid">
			<fieldset id="filter">
				<div class="col-sm-12">
					<label class="control-label">
						<strong class="f-head">Project Creation Summary:</strong>
					</label>
					<%= select_tag 'project_list_report', options_for_select(options_for_projects_select(true)) , :style=>"margin-top: 8px; margin-right: 10px; margin-left: 10px; width: 15%;" %>
					<label class="f-head" >Start Date Identified:</label>
					<input type="text" class="startDate" id="dateStartReport" required readonly%>
						<label class="f-head" >End Date Identified:</label>
					<input type="text" class="endDate" id="dateEndReport" required readonly %>
					<%=link_to "Find" , '#' , class: "btn btn-primary find_issues", data_pdf: false  %> 
					<%=link_to "PDF" , '#' , class: "btn btn-primary find_issues", data_pdf: true, target: "_blank" %> 
				</div>
				<div class="col-sm-8">
					
				</div>
			</fieldset>
			<div class="project_list_report_div" >
				<%= render "project_list_report" %>
			</div">	
	</div>  
</fieldset>